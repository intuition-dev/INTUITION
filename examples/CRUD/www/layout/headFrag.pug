script.
   var _start = Date.now()
   window.ENV = '#{ENV}' 

title #{title}
meta(charset='utf-8')
meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
script(src='https://cdn.jsdelivr.net/npm/johnnydepp@0.1.0/dist/depp.min.js')

link(rel='stylesheet', href='https://cdn.jsdelivr.net/gh/intuition-dev/toolBelt@v1.8.29/bootStrap/css/bootstrapTop.css')
script(src='https://cdn.jsdelivr.net/gh/intuition-dev/toolBelt@v1.8.29/toolBelt/toolBelt.js')

